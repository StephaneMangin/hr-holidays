<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="hr_employee_hour_tree_view" model="ir.ui.view">
        <field name="name">hr.employee.hour.tree</field>
        <field name="model">hr.employee.hour</field>
        <field
            name="inherit_id"
            ref="hr_timesheet_overview.hr_employee_hour_tree_view"
        />
